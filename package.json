{
  "name": "@namics/frontend-defaults",
  "version": "1.0.0",
  "description": "Some defaults for frontend projects",
  "license": "MIT",
  "repository": "namics/frontend-defaults",
  "author": "Namics Frontend Team",
  "engines": {
    "node": ">=6 <9",
    "npm": ">=3 <6"
  },
  "scripts": {
    "c": "node_modules/.bin/git-cz",
    "lint": "npm run lint:js",
    "lint:js": "node_modules/.bin/eslint .",
    "prettier": "node_modules/.bin/prettier --write '**/*.js' --config package.json",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "files": [
    "codequality",
    "doc",
    "editorconfig",
    "repo",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "codequality",
    "docs",
    "frontend",
    "editorconfig",
    "commitlint",
    "commitizen"
  ],
  "commitlint": {
    "extends": [
      "./repo/commitlint/commitlint-config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ]
  },
  "prettier": {
    "useTabs": true,
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babylon",
    "semi": true
  },
  "dependencies": {
    "import-from": "2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "6.0.2",
    "@commitlint/config-conventional": "6.0.2",
    "@commitlint/config-lerna-scopes": "6.0.2",
    "@namics/eslint-config": "4.2.0",
    "commitizen": "2.9.6",
    "conventional-changelog-cli": "1.3.5",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.15.0",
    "eslint-plugin-import": "2.8.0",
    "husky": "0.14.3",
    "lint-staged": "4.2.3",
    "prettier": "1.7.4"
  }
}
